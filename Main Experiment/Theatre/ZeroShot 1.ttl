
@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Cl_TheatreFestival rdf:type rdfs:Class .
:Cl_City rdf:type rdfs:Class .
:Cl_Play rdf:type rdfs:Class .
:Cl_Theatre rdf:type rdfs:Class .
:Cl_Country rdf:type rdfs:Class .
:Cl_Production rdf:type rdfs:Class .
:Cl_Ensemble rdf:type rdfs:Class .
:Cl_Author rdf:type rdfs:Class .
:Cl_Newspaper rdf:type rdfs:Class .
:Cl_Actor rdf:type rdfs:Class .
:Cl_Role rdf:type rdfs:Class .

:Cl_TheatreFestival rdfs:subClassOf :Cl_Event .
:Cl_Play rdfs:subClassOf :Cl_Artwork .
:Cl_Production rdfs:subClassOf :Cl_Event .
:Cl_Ensemble rdfs:subClassOf :Cl_Group .
:Cl_Author rdfs:subClassOf :Cl_Person .
:Cl_Actor rdfs:subClassOf :Cl_Person .
:Cl_Role rdfs:subClassOf :Cl_Character .

:Cl_FestivalPlay rdf:type rdfs:Class .
:Cl_ProductionPremier rdf:type rdfs:Class .
:Cl_ProductionReview rdf:type rdfs:Class .
:Cl_ActorEmployment rdf:type rdfs:Class .
:Cl_ActorRole rdf:type rdfs:Class .

:hasDate rdf:type owl:Property ; rdfs:domain :Cl_Event ; rdfs:range xsd:date .
:hasLocation rdf:type owl:Property ; rdfs:domain :Cl_Event ; rdfs:range :Cl_City .
:hasPlay rdf:type owl:Property ; rdfs:domain :Cl_FestivalPlay ; rdfs:range :Cl_Play .
:isLocatedIn rdf:type owl:Property ; rdfs:domain :Cl_Theatre ; rdfs:range :Cl_City .
:isInCountry rdf:type owl:Property ; rdfs:domain :Cl_City ; rdfs:range :Cl_Country .
:hasBasis rdf:type owl:Property ; rdfs:domain :Cl_Production ; rdfs:range :Cl_Play .
:hasMember rdf:type owl:Property ; rdfs:domain :Cl_Ensemble ; rdfs:range :Cl_Actor .
:wrotePlay rdf:type owl:Property ; rdfs:domain :Cl_Author ; rdfs:range :Cl_Play .
:hasWritingPeriod rdf:type owl:Property ; rdfs:domain :Cl_Play ; rdfs:range xsd:duration .
:hasActCount rdf:type owl:Property ; rdfs:domain :Cl_Play ; rdfs:range xsd:integer .
:hasPremier rdf:type owl:Property ; rdfs:domain :Cl_ProductionPremier ; rdfs:range xsd:date .
:hasStarRating rdf:type owl:Property ; rdfs:domain :Cl_ProductionReview ; rdfs:range xsd:integer .
:hasEmploymentStart rdf:type owl:Property ; rdfs:domain :Cl_ActorEmployment ; rdfs:range xsd:date .
:hasRole rdf:type owl:Property ; rdfs:domain :Cl_ActorRole ; rdfs:range :Cl_Role .

:Cl_FestivalPlay rdfs:subClassOf [ rdf:type owl:Restriction ; owl:onProperty :hasPlay ; owl:allValuesFrom :Cl_Play ] .
:Cl_ProductionPremier rdfs:subClassOf [ rdf:type owl:Restriction ; owl:onProperty :hasPremier ; owl:allValuesFrom xsd:date ] .
:Cl_ProductionReview rdfs:subClassOf [ rdf:type owl:Restriction ; owl:onProperty :hasStarRating ; owl:allValuesFrom xsd:integer ] .
:Cl_ActorEmployment rdfs:subClassOf [ rdf:type owl:Restriction ; owl:onProperty :hasEmploymentStart ; owl:allValuesFrom xsd:date ] .
:Cl_ActorRole rdfs:subClassOf [ rdf:type owl:Restriction ; owl:onProperty :hasRole ; owl:allValuesFrom :Cl_Role ] .
