    rdfs:range :Cl_Actor .

:hasRole a rdf:Property ;
    rdfs:domain :Cl_Actor ;
    rdfs:range :Cl_Role .

:hasReview a rdf:Property ;
    rdfs:domain :Cl_Production ;
    rdfs:range :Cl_Review .

:reviewedBy a rdf:Property ;
    rdfs:domain :Cl_Review ;
    rdfs:range :Cl_Newspaper .

:hasStartDate a rdf:Property ;
    rdfs:domain :Cl_Festival ;
    rdfs:range xsd:date .

:hasEndDate a rdf:Property ;
    rdfs:domain :Cl_Festival ;
    rdfs:range xsd:date .

:hasPremierDate a rdf:Property ;
    rdfs:domain :Cl_Production ;
    rdfs:range xsd:date .

:hasStarRating a rdf:Property ;
    rdfs:domain :Cl_Review ;
    rdfs:range xsd:integer .

:hasEmploymentStartDate a rdf:Property ;
    rdfs:domain :Cl_Actor ;
    rdfs:range xsd:date .

:hasNumberOfActs a rdf:Property ;
    rdfs:domain :Cl_Play ;
    rdfs:range xsd:integer .

:Cl_Production rdfs:subClassOf [
  rdf:type owl:Restriction ;
  owl:onProperty :hasPremierDate ;
  owl:cardinality "1"^^xsd:nonNegativeInteger
] .

:Cl_Play rdfs:subClassOf [
  rdf:type owl:Restriction ;
  owl:onProperty :hasNumberOfActs ;
  owl:minCardinality "1"^^xsd:nonNegativeInteger
] .

:Cl_Actor rdfs:subClassOf [
  rdf:type owl:Restriction ;
  owl:onProperty :hasEmploymentStartDate ;
  owl:cardinality "1"^^xsd:nonNegativeInteger
] .

:Cl_Review rdfs:subClassOf [
  rdf:type owl:Restriction ;
  owl:onProperty :hasStarRating ;
  owl:cardinality "1"^^xsd:nonNegativeInteger
] .
