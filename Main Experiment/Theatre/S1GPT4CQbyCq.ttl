@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Cl_Play a rdfs:Class .
:Cl_TheatreFestival a rdfs:Class .
:Cl_PlayPerformance a rdfs:Class ;
    rdfs:subClassOf :Cl_Play, :Cl_TheatreFestival .
:Cl_Theatre a rdfs:Class .
:Cl_City a rdfs:Class .
:Cl_Country a rdfs:Class .
:Cl_TheatreLocation a rdfs:Class ;
    rdfs:subClassOf :Cl_Theatre, :Cl_City .
:Cl_CityLocation a rdfs:Class ;
    rdfs:subClassOf :Cl_City, :Cl_Country .
:Cl_Production a rdfs:Class .
:Cl_PlayBasis a rdfs:Class ;
    rdfs:subClassOf :Cl_Play, :Cl_Production .
:Cl_Ensemble a rdfs:Class .
:Cl_Actor a rdfs:Class .
:Cl_Time a rdfs:Class .
:Cl_EnsembleMembership a rdfs:Class ;
    rdfs:subClassOf :Cl_Ensemble, :Cl_Actor, :Cl_Time .
:Cl_Author a rdfs:Class .
:Cl_TimePeriod a rdfs:Class .
:Cl_Authorship a rdfs:Class ;
    rdfs:subClassOf :Cl_Author, :Cl_Play, :Cl_TimePeriod .
:Cl_Act a rdfs:Class ;
    rdfs:subClassOf :Cl_Play .
:Cl_Premier a rdfs:Class ;
    rdfs:subClassOf :Cl_PlayPerformance .
:Cl_Review a rdfs:Class ;
    rdfs:subClassOf :Cl_PlayPerformance .
:Cl_Newspaper a rdfs:Class .
:Cl_Employment a rdfs:Class ;
    rdfs:subClassOf :Cl_Actor .
:Cl_Role a rdfs:Class ;
    rdfs:subClassOf :Cl_Actor .

:performedDuring a rdf:Property ;
    rdfs:domain :Cl_PlayPerformance ;
    rdfs:range :Cl_TheatreFestival .
:isPerformanceOf a rdf:Property ;
    rdfs:domain :Cl_PlayPerformance ;
    rdfs:range :Cl_Play .
:locatedIn a rdf:Property ;
    rdfs:domain :Cl_TheatreLocation ;
    rdfs:range :Cl_City .
:isLocationOf a rdf:Property ;
    rdfs:domain :Cl_TheatreLocation ;
    rdfs:range :Cl_Theatre .
:cityLocatedIn a rdf:Property ;
    rdfs:domain :Cl_CityLocation ;
    rdfs:range :Cl_Country .
:isCityLocationOf a rdf:Property ;
    rdfs:domain :Cl_CityLocation ;
    rdfs:range :Cl_City .
:isBasisOf a rdf:Property ;
    rdfs:domain :Cl_PlayBasis ;
    rdfs:range :Cl_Production .
:hasBasis a rdf:Property ;
    rdfs:domain :Cl_PlayBasis ;
    rdfs:range :Cl_Play .
:isMemberOf a rdf:Property ;
    rdfs:domain :Cl_EnsembleMembership ;
    rdfs:range :Cl_Ensemble .
:hasMember a rdf:Property ;
    rdfs:domain :Cl_EnsembleMembership ;
    rdfs:range :Cl_Actor .
:atTime a rdf:Property ;
    rdfs:domain :Cl_EnsembleMembership ;
    rdfs:range :Cl_Time .
:wrote a rdf:Property ;
    rdfs:domain :Cl_Authorship ;
    rdfs:range :Cl_Play .
:hasAuthor a rdf:Property ;
    rdfs:domain :Cl_Authorship ;
    rdfs:range :Cl_Author .
:hasTimePeriod a rdf:Property ;
    rdfs:domain :Cl_Authorship ;
    rdfs:range :Cl_TimePeriod .
:hasAct a rdf:Property ;
    rdfs:domain :Cl_Play ;
    rdfs:range :Cl_Act .
:numberOfActs a rdf:Property ;
    rdfs:domain :Cl_Act ;
    rdfs:range xsd:integer .
:hasPremier a rdf:Property ;
    rdfs:domain :Cl_PlayPerformance ;
    rdfs:range :Cl_Premier .
:dateOfPremier a rdf:Property ;
    rdfs:domain :Cl_Premier ;
    rdfs:range xsd:date .
:hasReview a rdf:Property ;
    rdfs:domain :Cl_PlayPerformance ;
    rdfs:range :Cl_Review .
:starRating a rdf:Property ;
    rdfs:domain :Cl_Review ;
    rdfs:range xsd:integer .
:givenBy a rdf:Property ;
    rdfs:domain :Cl_Review ;
    rdfs:range :Cl_Newspaper .
:employedAt a rdf:Property ;
    rdfs:domain :Cl_Employment ;
    rdfs:range :Cl_Theatre .
:startDate a rdf:Property ;
    rdfs:domain :Cl_Employment ;
    rdfs:range xsd:date .
:hasRole a rdf:Property ;
    rdfs:domain :Cl_Role ;
    rdfs:range :Cl_Production .
:roleTime a rdf:Property ;
    rdfs:domain :Cl_Role ;
    rdfs:range xsd:date .