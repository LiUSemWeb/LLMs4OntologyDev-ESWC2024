@prefix : <http://www.example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

# Classes
:Cl_Person a rdfs:Class .
:Cl_Instrument a rdfs:Class .
:Cl_Band a rdfs:Class .
:Cl_Time a rdfs:Class .
:Cl_Role a rdfs:Class .
:Cl_Album a rdfs:Class .
:Cl_Song a rdfs:Class .
:Cl_Track a rdfs:Class .
:Cl_Critic a rdfs:Class .
:Cl_Performance a rdfs:Class .
:Cl_Location a rdfs:Class .
:Cl_City a rdfs:Class .
:Cl_Region a rdfs:Class .
:Cl_Country a rdfs:Class .
:Cl_CityRegion a rdfs:Class ;
    rdfs:subClassOf :Cl_City, :Cl_Region .
:Cl_RegionCountry a rdfs:Class ;
    rdfs:subClassOf :Cl_Region, :Cl_Country .
:Cl_PerformanceLocation a rdfs:Class ;
    rdfs:subClassOf :Cl_Performance, :Cl_Location .
:Cl_PerformanceSong a rdfs:Class ;
    rdfs:subClassOf :Cl_Performance, :Cl_Song .
:Cl_PersonInstrumentPlay a rdfs:Class ;
    rdfs:subClassOf :Cl_Person, :Cl_Instrument .
:Cl_BandMemberTime a rdfs:Class ;
    rdfs:subClassOf :Cl_Band, :Cl_Person, :Cl_Time .
:Cl_PersonBandRoleTime a rdfs:Class ;
    rdfs:subClassOf :Cl_Band, :Cl_Person, :Cl_Role, :Cl_Time .
:Cl_BandAlbumTime a rdfs:Class ;
    rdfs:subClassOf :Cl_Band, :Cl_Album, :Cl_Time .
:Cl_TrackSongAlbum a rdfs:Class ;
    rdfs:subClassOf :Cl_Track, :Cl_Song, :Cl_Album .
:Cl_SongCompositionTime a rdfs:Class ;
    rdfs:subClassOf :Cl_Song, :Cl_Time .
:Cl_CriticAlbumOpinion a rdfs:Class ;
    rdfs:subClassOf :Cl_Critic, :Cl_Album .
:Cl_BandPerformanceTime a rdfs:Class ;
    rdfs:subClassOf :Cl_Band, :Cl_Performance, :Cl_Time .

# Object Properties
:playsInstrument a rdf:Property ;
    rdfs:domain :Cl_Person ;
    rdfs:range :Cl_PersonInstrumentPlay .

:playedBy a rdf:Property ;
    rdfs:domain :Cl_Instrument ;
    rdfs:range :Cl_PersonInstrumentPlay .

:hasMember a rdf:Property ;
    rdfs:domain :Cl_Band ;
    rdfs:range :Cl_BandMemberTime .

:memberOf a rdf:Property ;
    rdfs:domain :Cl_Person ;
    rdfs:range :Cl_BandMemberTime .

:hasRole a rdf:Property ;
    rdfs:domain :Cl_Person ;
    rdfs:range :Cl_PersonBandRoleTime .

:recordedAlbum a rdf:Property ;
    rdfs:domain :Cl_Band ;
    rdfs:range :Cl_BandAlbumTime .

:isTrackOf a rdf:Property ;
    rdfs:domain :Cl_Track ;
    rdfs:range :Cl_TrackSongAlbum .

:hasOpinionOn a rdf:Property ;
    rdfs:domain :Cl_Critic ;
    rdfs:range :Cl_CriticAlbumOpinion .

:performedOn a rdf:Property ;
    rdfs:domain :Cl_Band ;
    rdfs:range :Cl_BandPerformanceTime .

:playedSong a rdf:Property ;
    rdfs:domain :Cl_Performance ;
    rdfs:range :Cl_PerformanceSong .

:takesPlaceAt a rdf:Property ;
    rdfs:domain :Cl_Performance ;
    rdfs:range :Cl_PerformanceLocation .

:locatedIn a rdf:Property ;
    rdfs:domain :Cl_City ;
    rdfs:range :Cl_CityRegion .

:isInCountry a rdf:Property ;
    rdfs:domain :Cl_Region ;
    rdfs:range :Cl_RegionCountry .

# Data Properties
:hasName a rdf:Property ;
    rdfs:domain :Cl_Person, :Cl_Instrument, :Cl_Band, :Cl_Role, :Cl_Album, :Cl_Song, :Cl_Track, :Cl_Critic, :Cl_Performance, :Cl_Location, :Cl_City, :Cl_Region, :Cl_Country ;
    rdfs:range xsd:string .

:atTime a rdf:Property ;
    rdfs:domain :Cl_BandMemberTime, :Cl_PersonBandRoleTime, :Cl_BandAlbumTime, :Cl_BandPerformanceTime ;
    rdfs:range xsd:dateTime .

:hasTracks a rdf:Property ;
    rdfs:domain :Cl_Album ;
    rdfs:range xsd:integer .

:releasedOn a rdf:Property ;
    rdfs:domain :Cl_Album ;
    rdfs:range xsd:dateTime .

:composedOn a rdf:Property ;
    rdfs:domain :Cl_Song ;
    rdfs:range xsd:dateTime .

:opinion a rdf:Property ;
    rdfs:domain :Cl_CriticAlbumOpinion ;
    rdfs:range xsd:string .

# Restrictions
:Cl_Album owl:equivalentClass [
    rdf:type owl:Restriction ;
    owl:onProperty :hasTracks ;
    owl:minCardinality 1
] .